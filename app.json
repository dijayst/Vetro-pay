{
  "expo": {
    "name": "Vetropay Mobile",
    "slug": "vetropay-mobile",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.4.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#256cdc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.anclatechs.vetropaymobile",
      "versionCode": 9,
      "useNextNotificationsApi": true,
      "permissions": ["USE_BIOMETRIC", "USE_FINGERPRINT", "READ_CONTACTS", "CAMERA"]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000066",
      "translucent": true
    },
    "facebookScheme": "fb713268446200988",
    "facebookAppId": "713268446200988",
    "facebookDisplayName": "VetroPay Mobile",
    "facebookAutoLogAppEventsEnabled": true,
    "description": ""
  }
}
