{
  "expo": {
    "scheme": "vetropay",
    "name": "Vetropay Mobile",
    "slug": "vetropay-mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.19",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/notification-icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#256cdc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes of for faster payment.",
        "NSMicrophoneUsageDescription": "This app does not require access to the microphone.",
        "NSFaceIDUsageDescription": "This app uses the Face ID as an alt method to authenticate user berfore logging user in",
        "NSContactsUsageDescription": "This app uses contact list to make purchase airtime faster"
      },
      "bundleIdentifier": "com.anclatechs.vetropaymobile",
      "buildNumber": "1.0.19"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.anclatechs.vetropaymobile",
      "versionCode": 30,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "READ_CONTACTS",
        "CAMERA"
      ],
      "blockedPermissions": [
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000066",
      "translucent": true
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "969b74fb-e7a7-48ca-99cd-db4a3c4684ee"
      }
    }
  }
}